{
  "manifest_version": 3, "name": "Cookie Editor", "version": "1.0.0", "description": "View and edit cookies",
  "permissions": ["storage", "cookies"],
  "host_permissions": ["<all_urls>"],
  "action": { "default_title": "Cookie Editor", "default_popup": "popup.html", "default_icon": { "16": "icons/icon16.png", "48": "icons/icon48.png", "128": "icons/icon128.png" } },
  "icons": { "16": "icons/icon16.png", "48": "icons/icon48.png", "128": "icons/icon128.png" }
}
