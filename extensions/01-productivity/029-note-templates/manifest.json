{
  "manifest_version": 3,
  "name": "Note Templates",
  "version": "1.0.0",
  "description": "Pre-built templates for meetings, journals, reports, and more. Support custom templates with variables.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Note Templates"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
