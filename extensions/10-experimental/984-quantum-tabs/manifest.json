{
  "manifest_version": 3,
  "name": "Quantum Tabs",
  "version": "1.0.0",
  "description": "Revolutionizes tab management with quantum-inspired concepts like superposition and entanglement.",
  "permissions": [
    "tabs",
    "storage",
    "history"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
